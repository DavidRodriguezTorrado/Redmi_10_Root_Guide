<?xml version="1.0" encoding="utf-8"?>
<Window>
  <VerticalLayout name="dev_list_window">
    <List name="dev_list" itemhlinesize="1" itemvlinesize="1" itemhlinecolor="#dfe9f5" itemvlinecolor="#dfe9f5"
          itemalign="center" bottombordersize="2">
      <ListHeader height="24">
        <ListHeaderItem text="No." width="40" sepwidth="1"/>
        <ListHeaderItem text="SN" width="160" sepwidth="1"/>
        <ListHeaderItem text="Product" width="160" sepwidth="1"/>
        <ListHeaderItem text="Status" width="100" sepwidth="1"/>
        <ListHeaderItem text="Message" sepwidth="1"/>
      </ListHeader>
    </List>
    <HorizontalLayout height="36" width="400">
      <Button name="scan_device_btn" text="Refresh(F5)" width="115" font="14" padding="0,0,10,0"/>
      <Button name="unlock_device_btn" text="Unlock(F6)" width="115" font="14" padding="10,0,0,0"/>
      <Button name="violence_test_btn" visible="false" text="TEST" font="14" padding="10,0,0,0"/>
    </HorizontalLayout>
  </VerticalLayout>
</Window>
